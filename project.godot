SignalBus="*res://globals/signal_bus.gd"
Globals="*res://globals/globals.gd"
HexFunctions="*res://globals/hex_functions.gd"

[display]

window/size/viewport_height=1288
window/stretch/mode="canvas_items"

[global_group]

pieces=""
